{
  "version": 2,
  "env": {},
  "builds": [
    {
      "src": "./bin/www",
      "use": "@vercel/node"
    },
    {
      "src": "ui/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./bin/www"
    },
    {
      "src": "/",
      "dest": "./public/index.html"
    }
  ]
}
